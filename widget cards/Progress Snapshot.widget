{
  "version": "1.0",
  "name": "Progress Snapshot",
  "template": "{\"type\":\"Card\",\"size\":\"md\",\"background\":{{ (accents.background) | tojson }},\"theme\":\"dark\",\"children\":[{\"type\":\"Col\",\"gap\":3,\"children\":[{\"type\":\"Row\",\"gap\":2,\"align\":\"center\",\"children\":[{\"type\":\"Box\",\"size\":28,\"radius\":\"sm\",\"background\":{{ (accents.primary) | tojson }},\"align\":\"center\",\"justify\":\"center\",\"children\":[{\"type\":\"Icon\",\"name\":\"analytics\",\"color\":\"white\",\"size\":\"md\"}]},{\"type\":\"Title\",\"value\":{{ (title) | tojson }},\"size\":\"md\"},{\"type\":\"Spacer\"},{\"type\":\"Badge\",\"label\":\"Weekly\",\"color\":\"info\",\"variant\":\"soft\"}]},{\"type\":\"Chart\",\"height\":140,\"data\":{{ (chartData) | tojson }},\"series\":[{\"type\":\"Text\",\"value\":{\"type\":\"line\",\"dataKey\":\"value\",\"color\":{{ (accents.primary) | tojson }},\"curveType\":\"monotoneX\"}}],\"xAxis\":{\"dataKey\":\"day\"},\"showYAxis\":true},{\"type\":\"Divider\",\"color\":\"subtle\"},{\"type\":\"Col\",\"gap\":1,\"children\":[{\"type\":\"Text\",\"value\":\"Weekly Goal\",\"size\":\"sm\",\"color\":\"secondary\"},{\"type\":\"Row\",\"children\":[{\"type\":\"Title\",\"value\":{{ ((percentComplete ~ \"% complete\")) | tojson }},\"size\":\"2xl\",\"weight\":\"bold\",\"color\":{\"light\":{{ (accents.primary) | tojson }},\"dark\":{{ (accents.primary) | tojson }}}},{\"type\":\"Spacer\"},{\"type\":\"Badge\",\"label\":{{ ((streakDays ~ \" day streak\")) | tojson }},\"color\":\"warning\",\"variant\":\"soft\"}]}]},{\"type\":\"Button\",\"label\":{{ (buttonLabel) | tojson }},\"style\":\"primary\",\"onClickAction\":{\"type\":\"report.view\"}}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "chartData": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "day": {
              "type": "string"
            },
            "value": {
              "type": "number"
            }
          },
          "required": [
            "day",
            "value"
          ],
          "additionalProperties": false
        }
      },
      "percentComplete": {
        "type": "number"
      },
      "streakDays": {
        "type": "number"
      },
      "buttonLabel": {
        "type": "string"
      },
      "accents": {
        "type": "object",
        "properties": {
          "background": {
            "type": "string"
          },
          "primary": {
            "type": "string"
          },
          "secondary": {
            "type": "string"
          }
        },
        "required": [
          "background",
          "primary",
          "secondary"
        ],
        "additionalProperties": false
      }
    },
    "required": [
      "title",
      "chartData",
      "percentComplete",
      "streakDays",
      "buttonLabel",
      "accents"
    ],
    "additionalProperties": false
  },
  "outputJsonPreview": {
    "type": "Card",
    "size": "md",
    "background": "#0B0B0B",
    "theme": "dark",
    "children": [
      {
        "type": "Col",
        "gap": 3,
        "children": [
          {
            "type": "Row",
            "gap": 2,
            "align": "center",
            "children": [
              {
                "type": "Box",
                "size": 28,
                "radius": "sm",
                "background": "#FF5A1F",
                "align": "center",
                "justify": "center",
                "children": [
                  {
                    "type": "Icon",
                    "name": "analytics",
                    "color": "white",
                    "size": "md"
                  }
                ]
              },
              {
                "type": "Title",
                "value": "Progress Snapshot",
                "size": "md"
              },
              {
                "type": "Spacer"
              },
              {
                "type": "Badge",
                "label": "Weekly",
                "color": "info",
                "variant": "soft"
              }
            ]
          },
          {
            "type": "Chart",
            "height": 140,
            "data": [
              {
                "day": "Sun",
                "value": 120
              },
              {
                "day": "Mon",
                "value": 180
              },
              {
                "day": "Tue",
                "value": 195
              },
              {
                "day": "Wed",
                "value": 205
              },
              {
                "day": "Thu",
                "value": 320
              },
              {
                "day": "Fri",
                "value": 280
              },
              {
                "day": "Sat",
                "value": 350
              }
            ],
            "series": [
              {
                "type": "line",
                "dataKey": "value",
                "color": "#FF5A1F",
                "curveType": "monotoneX"
              }
            ],
            "xAxis": {
              "dataKey": "day"
            },
            "showYAxis": true
          },
          {
            "type": "Divider",
            "color": "subtle"
          },
          {
            "type": "Col",
            "gap": 1,
            "children": [
              {
                "type": "Text",
                "value": "Weekly Goal",
                "size": "sm",
                "color": "secondary"
              },
              {
                "type": "Row",
                "children": [
                  {
                    "type": "Title",
                    "value": "85% complete",
                    "size": "2xl",
                    "weight": "bold",
                    "color": {
                      "light": "#FF5A1F",
                      "dark": "#FF5A1F"
                    }
                  },
                  {
                    "type": "Spacer"
                  },
                  {
                    "type": "Badge",
                    "label": "5 day streak",
                    "color": "warning",
                    "variant": "soft"
                  }
                ]
              }
            ]
          },
          {
            "type": "Button",
            "label": "View Full Report",
            "style": "primary",
            "onClickAction": {
              "type": "report.view"
            }
          }
        ]
      }
    ]
  },
