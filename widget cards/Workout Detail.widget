{
  "version": "1.0",
  "name": "Workout Detail",
  "template": "{\"type\":\"Card\",\"size\":\"md\",\"background\":{{ (background) | tojson }},\"theme\":\"dark\",\"confirm\":{\"label\":\"Start Session\",\"action\":{\"type\":\"workout.start\",\"payload\":{\"id\":{{ (id) | tojson }}}}},\"children\":[{\"type\":\"Col\",\"gap\":3,\"children\":[{\"type\":\"Image\",\"src\":{{ (heroImage) | tojson }},\"alt\":\"Workout hero\",\"aspectRatio\":{{ ((16 / 9)) | tojson }},\"flush\":true},{\"type\":\"Col\",\"gap\":1,\"children\":[{\"type\":\"Title\",\"value\":{{ (title) | tojson }},\"size\":\"lg\",\"color\":{{ (accentPrimary) | tojson }}},{\"type\":\"Row\",\"gap\":4,\"align\":\"center\",\"children\":[{\"type\":\"Row\",\"gap\":1,\"align\":\"center\",\"children\":[{\"type\":\"Icon\",\"name\":\"calendar\",\"color\":{{ (accentPrimary) | tojson }}},{\"type\":\"Caption\",\"value\":{{ (duration) | tojson }}}]},{\"type\":\"Row\",\"gap\":1,\"align\":\"center\",\"children\":[{\"type\":\"Icon\",\"name\":\"bolt\",\"color\":{{ (accentPrimary) | tojson }}},{\"type\":\"Caption\",\"value\":{{ (difficulty) | tojson }}}]},{\"type\":\"Row\",\"gap\":1,\"align\":\"center\",\"children\":[{\"type\":\"Icon\",\"name\":\"cube\",\"color\":{{ (accentPrimary) | tojson }}},{\"type\":\"Caption\",\"value\":{{ (equipment) | tojson }}}]}]}]},{\"type\":\"Row\",\"children\":[{\"type\":\"Button\",\"label\":\"Play video\",\"iconStart\":\"play\",\"variant\":\"outline\",\"onClickAction\":{\"type\":\"video.open\",\"payload\":{\"url\":{{ (videoUrl) | tojson }},\"id\":{{ (id) | tojson }}}}},{\"type\":\"Spacer\"},{\"type\":\"Badge\",\"label\":\"Tips\",\"color\":\"discovery\"}]},{\"type\":\"Col\",\"gap\":2,\"children\":[{%- set _c -%}{%-for t in tips -%}{%-set i = loop.index0 -%},{\"type\":\"Row\",\"key\":{{ ((\"tip-\" ~ i)) | tojson }},\"gap\":2,\"align\":\"center\",\"children\":[{\"type\":\"Icon\",\"name\":\"lightbulb\",\"color\":{{ (accentSecondary) | tojson }}},{\"type\":\"Text\",\"value\":{{ (t) | tojson }},\"size\":\"sm\",\"color\":\"secondary\"}]}{%-endfor-%}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "title": {
        "type": "string"
      },
      "heroImage": {
        "type": "string",
        "format": "uri"
      },
      "duration": {
        "type": "string"
      },
      "difficulty": {
        "type": "string"
      },
      "equipment": {
        "type": "string"
      },
      "videoUrl": {
        "type": "string",
        "format": "uri"
      },
      "tips": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "background": {
        "type": "string"
      },
      "accentPrimary": {
        "type": "string"
      },
      "accentSecondary": {
        "type": "string"
      }
    },
    "required": [
      "id",
      "title",
      "heroImage",
      "duration",
      "difficulty",
      "equipment",
      "videoUrl",
      "tips",
      "background",
      "accentPrimary",
      "accentSecondary"
    ],
    "additionalProperties": false
  },
